							jQuery("#tblDotDK tr").each(function(){
								jQuery(this).removeClass("active");
								jQuery(this).find(".fa").removeClass("fa-long-arrow-right");
							});
							jQuery(el).find(".fa").addClass("fa-long-arrow-right");
