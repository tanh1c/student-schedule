	﻿<script>
var loadingText = '<div class="overlay"><i class="fa fa-refresh fa-spin"></i></div>';
function mask(elId){

	if(elId != ""){
		//neu elId co ton tai
		if(jQuery("#"+ elId).length > 0){
			//neu elId.html() la empty
			if(jQuery("#"+ elId).html().trim() == ""){
				jQuery("#"+ elId).css('min-height', '50px');
			}
			
			jQuery("#"+ elId).ploading({
			action: 'show'
			});
		}
		
	}else{
			jQuery('body').ploading({
			action: 'show'
			});
	}
	
	
}

function unmask(elId){
	if(elId != ""){
		if(jQuery("#"+ elId).length > 0){
			jQuery("#"+ elId + "").ploading({action: 'hide'});
		}
		
	}else{
			jQuery("body").ploading({action: 'hide'});
	}
}

/****************************
*	GROUPING TABLE BY ROW
*****************************/
	function group(jQueryrows, columnIndexGroup, prev_ColumnIndexGroup){
		if (columnIndexGroup === -1){
			return;
		}
		var i , currentIndex = columnIndexGroup, count=1, lst=[];
		var tds = jQueryrows.find('td:eq('+ currentIndex +')');
		var ctrl = jQuery(tds[0]);
		
		var maxCount = 0;
		if(prev_ColumnIndexGroup != -1){
			maxCount = ctrl.closest('tr').find('td:eq('+ prev_ColumnIndexGroup +')').attr('rowspan');
		}
	
		var totalCount = 0;
		for (i=1;i<=tds.length;i++){
			totalCount++;
			
			if(prev_ColumnIndexGroup != -1 && totalCount >= maxCount){
				ctrl.attr('rowspan',count);
				
				count=1;
				lst = [];
				ctrl=jQuery(tds[i]);

				if(prev_ColumnIndexGroup != -1){			
					maxCount = ctrl.closest('tr').find('td:eq('+ prev_ColumnIndexGroup +')').attr('rowspan');
				}
				
				totalCount = 0;
			}
			else{
				//console.log(ctrl.text().trim() + '== ' + jQuery(tds[i]).text().trim());
				if (ctrl.text().trim() ==  jQuery(tds[i]).text().trim()){
					count++;
					jQuery(tds[i]).addClass('deleted');

				}
				else{
					ctrl.attr('rowspan',count);
					
					count=1;
					lst = [];
					ctrl=jQuery(tds[i]);
				}
			}

		}
	}

	function groupTable(tableId, columns){
		
		for(var i = 0; i < columns.length; i++){
			if(i == 0)
				group(jQuery('#'+tableId+ ' tr:has(td)'),columns[i],-1);
			else
				group(jQuery('#'+tableId+ ' tr:has(td)'),columns[i],columns[i-1]);
	    }
		
		
		jQuery('#'+tableId+ ' .deleted').remove();
		
	}
	
function logout(){
	if(jQuery("input[name='dkmh_XacNhanYN']").length  > 0){
		jQuery.msgBox({
			title: "CẢNH BÁO",
			content: "BẠN CHƯA XÁC NHẬN KẾT QUẢ ĐĂNG KÝ!<br/> Tiếp tục đăng xuất?",
			type: "confirm",
			opacity: 0.5,
			buttons: [{ value: "Đồng ý" }, { value: "Không" }],
			success: function (result) {
				if (result == "Đồng ý") {
					document.location.href="logout.action";
				}
			}
		});
	}else{
		if(confirm("Bạn muốn đăng xuất khỏi hệ thống?")){
			document.location.href="logout.action";
		}
	}
}

function getAAOUrl(responseId, dotDKId){
			var url="getAAOUrl.action";
			var dataString = "dotDKId=" + dotDKId;

			mask(responseId);
			
			jQuery.ajax({
				type: "POST",
				url: url,
				data: dataString,
				success: function(response) {
					jQuery("#" + responseId).html(response);
					unmask(responseId);
										
				},
				error: function(){
					
					unmask(responseId);
					notify('error', 'Có lỗi xảy ra, vui lòng thử lại!!!');
				}
			});
}


	function notify(clazz,msg){

		switch (clazz.toLowerCase())
				{
					case 'success' : 
						jQuery.notify({
							icon: "fa fa-check",
							message: msg
						},{
							type: 'info',
						});
						break;
					case 'notice' : 
					case 'notice1' : 
					case 'warning' : 
						jQuery.notify({
							icon: "fa fa-check",
							message: msg
						},{
							type: 'warning',
						});
						break;
					case 'error' : 
						jQuery.notify({
							icon: "fa fa-ban",
							message: msg
						},{
							type: 'danger',
							newest_on_top: true,
							delay: 10000,
						});
						break;
					
				}
	}

