	function hieuChinhKetQuaDangKy(ketquaId, nlmhId, monHocId, el){
		var url="hieuChinhKetQuaDangKy.action";
		var dataString = "ketquaId=" + ketquaId;
		dataString += "&nlmhId=" + nlmhId;
		mask("tblNhomLopMonHocHieuChinh");
			jQuery.ajax({
				type: "POST",
				url: url,
				data: dataString,
				success: function(response) {
					unmask("tblNhomLopMonHocHieuChinh");
					var obj = jQuery.parseJSON(response);
					if(obj.code == "SUCCESS"){
						getKetQuaDangKy();
						notify(obj.code,obj.msg);
						jQuery("#hieuChinhKetQuaDangKyModal").modal("hide");
						
						jQuery("[group=input" +monHocId+ "]").removeClass("hide");
						jQuery("[group=img" +monHocId+ "]").addClass("hide");
						
						jQuery(el).parent().children().toggleClass("hide");
												
